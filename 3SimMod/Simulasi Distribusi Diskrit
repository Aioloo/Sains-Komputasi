import random

def discrete_distribution_simulation(outcomes, probabilities, num_samples):
    samples = random.choices(outcomes, probabilities, k=num_samples)
    return samples

outcomes = ["1", "2", "3", "4", "5", "6"]  # Hasil dadu
probabilities = [1/6] * 6  # Distribusi seragam
num_samples = 100

samples = discrete_distribution_simulation(outcomes, probabilities, num_samples)
print("Hasil simulasi distribusi diskrit:", samples)
